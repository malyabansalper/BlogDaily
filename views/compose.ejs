
<%- include("partials/header"); -%>
    <div class="card" >
      <h1 class="card-header" style="text-align: center;">Compose</h1><hr class="rounded">
      <p class="card-text">
        <form class="" action="/compose" method="post">
          <div class="form-group">
            <label>Title</label>
            <input class="form-control" type="text" name="postTitle">
            <label>Post</label>
            <textarea class="form-control" name="postBody" rows="5" cols="30"></textarea>
          </div>
          <button class="btn btn-custom" type="submit" name="button">Publish</button>
        </form>
      </p>
    </div>
    

<%- include("partials/footer"); -%>


